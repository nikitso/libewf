<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>LibEWF.Native</id>
    <version>9999.8888.7777</version>
    <title>libewf Native Library</title>
    <authors>@Company@</authors>
    <owners>@Company@</owners>
    <projectUrl>@ProjectUrl@</projectUrl>
    <description>Native C library for reading and writing Expert Witness Compression Format (EWF) files. This package contains the compiled DLL and static libraries for both x86 and x64 platforms.</description>
    <releaseNotes>Release based on libewf version 20201210</releaseNotes>
    <copyright>@Copyright@</copyright>
    <tags>native ewf forensics evidence compression nativepackage</tags>
  </metadata>
  <files>
    <!-- Header files -->
    <file src="..\..\..\include\libewf.h" target="build\native\include\libewf.h" />
    <file src="..\..\..\include\libewf\codepage.h" target="build\native\include\libewf\codepage.h" />
    <file src="..\..\..\include\libewf\definitions.h" target="build\native\include\libewf\definitions.h" />
    <file src="..\..\..\include\libewf\error.h" target="build\native\include\libewf\error.h" />
    <file src="..\..\..\include\libewf\extern.h" target="build\native\include\libewf\extern.h" />
    <file src="..\..\..\include\libewf\features.h" target="build\native\include\libewf\features.h" />
    <file src="..\..\..\include\libewf\types.h" target="build\native\include\libewf\types.h" />
    
    <!-- x64 Release binaries -->
    <file src="..\..\Release\x64\libewf.dll" target="build\native\bin\Release\x64\libewf.dll" />
    <file src="..\..\Release\x64\zlib1.dll" target="build\native\bin\Release\x64\zlib1.dll" />
    <file src="..\..\Release\x64\libewf.lib" target="build\native\lib\Release\x64\libewf.lib" />
    
    <!-- x86 Release binaries -->
    <file src="..\..\Release\x86\libewf.dll" target="build\native\bin\Release\Win32\libewf.dll" />
    <file src="..\..\Release\x86\zlib1.dll" target="build\native\bin\Release\Win32\zlib1.dll" />
    <file src="..\..\Release\x86\libewf.lib" target="build\native\lib\Release\Win32\libewf.lib" />

    <!-- x64 Debug binaries -->
    <file src="..\..\Debug\x64\libewf.dll" target="build\native\bin\Debug\x64\libewf.dll" />
    <file src="..\..\Debug\x64\libewf.pdb" target="build\native\bin\Debug\x64\libewf.pdb" />
    <file src="..\..\Debug\x64\zlibd1.dll" target="build\native\bin\Debug\x64\zlibd1.dll" />
    <file src="..\..\Debug\x64\libewf.lib" target="build\native\lib\Debug\x64\libewf.lib" />

    <!-- x86 Debug binaries -->
    <file src="..\..\Debug\x86\libewf.dll" target="build\native\bin\Debug\Win32\libewf.dll" />
    <file src="..\..\Debug\x86\libewf.pdb" target="build\native\bin\Debug\Win32\libewf.pdb" />
    <file src="..\..\Debug\x86\zlibd1.dll" target="build\native\bin\Debug\Win32\zlibd1.dll" />
    <file src="..\..\Debug\x86\libewf.lib" target="build\native\lib\Debug\Win32\libewf.lib" />

    <!-- MSBuild targets file -->
    <file src="libewf.targets" target="build\native\libewf.native.targets" />
  </files>
</package>
